, rgba(0,0,0,1) 100%);\r\n}\r\n`\r\nlet string2 = ''\r\n// string = string.replace(/\\n/g, \"<br>\")   //正则表达式回车换'<br>'\r\n// console.log(string.length) //打出length长度\r\nlet n = 0\r\n\r\nlet step = () => {\r\n  setTimeout(() => {\r\n    if (string[n] === '\\n') {\r\n      string2 += '<br>'\r\n      //如果是回车就改成<br>\r\n    } else if (string[n] === ' ') {\r\n      string2 += '&nbsp;'\r\n      //如果是空格就改成'&nbsp;'\r\n    } else {\r\n      //如果不是回车就照搬\r\n      string2 += string[n]\r\n    }\r\n    // string2 += string[n] === '\\n' ? '<br>' : string[n]\r\n    html.innerHTML = string2\r\n    style.innerHTML = string.substring(0, n)\r\n    //CSS就不能直接获取string2了，否则会出现 HTML 代码\r\n    window.scrollTo(0, 99999)//竖向滚动条\r\n    html.scrollTo(0, 99999)\r\n    if (n < string.length - 1) {\r\n      // 如果 n 不是最后一个，就继续\r\n      n += 1\r\n      step()\r\n    }\r\n  }, 1)\r\n}\r\nstep() //1=>2\r\n"},"lineCount":null}},"error":null,"hash":"d0067be3db7f63eb480ecf5e97b0693b","cacheData":{"env":{}}}